#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,s,limit;
	setbuf(stdout,NULL);
	printf("enter a number");
	scanf("%d",&limit);
	for(i=1;i<limit;i++){if(i%2!=0){
		s=i+i;
		printf("%d+%d=%d",i,i,s);}
	}
	return EXIT_SUCCESS;
}
