package paternPrinting;

import java.util.Scanner;

public class Paternsample {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter Limit");
		int num=sc.nextInt();
		int i,j;
		for(i=0;i<num;i++) {
			for(j=0;j<num;j++) {
		System.out.println("* ");}}
	}

}
